{
  "name": "party-lights",
  "version": "0.0.0",
  "description": "Party lights Tessel project",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src/index.js -o index.dist.js",
    "build.dist": "babel -d dist --ignore *.test.js src",
    "t2.run": "npm run build && t2 run dist/index.js",
    "test": "mocha --compilers js:babel-core/register --watch-extensions js src/.mocha.js \"src/*.test.js\""
  },
  "repository": "https://github.com/qimyi/party-lights",
  "keywords": [
    "Tessel"
  ],
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "tessel": "^0.3.25"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  }
}
